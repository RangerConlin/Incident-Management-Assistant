{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ICS 205 Communications Plan",
  "type": "object",
  "required": ["incident", "operational_period"],
  "properties": {
    "incident": {
      "type": "object",
      "required": ["name"],
      "properties": {
        "name": {"type": "string"}
      }
    },
    "operational_period": {
      "type": "object",
      "required": ["label"],
      "properties": {
        "label": {"type": "string"}
      }
    }
  }
}
